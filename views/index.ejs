                     <div class="copy-link">
                        <form action="">
                            <div class="url">
                                 <input type="url" placeholder="Nhập link youtube ở đây để bắt đầu..." class="animate-flip" id="url-input" name="videoUrl" value="">
                                
                                <div class="animate-flip">
                                    <div class="loader">
                                    </div>
                                    <div class="failed"><i class="fa fa-times-circle-o" aria-hidden="true"></i></div>
                                    <div class="success"><i class="fa fa-check-circle-o" aria-hidden="true"></i></div>
                                </div>
                            </div>
                            <input type="button" onclick="createGif()" value="Tạo ảnh"  class="btn-submit hidden">
                        </form>
                     </div>
                     
                     <div class="hidden-home  video-container hidden">
                        <div class="active-video">
                            <div id="player" class="overplay">
                            </div>
                            <div class="controls">
                                <button class="play playing">
                                    <i class="fa fa-caret-right click-active " aria-hidden="true"></i>
                                    <i class="fa fa-pause click-hidden" aria-hidden="true"></i>
                                </button>
                                <div class="select-time-start timeline">
                                    <div class="selection">
                                        <div class="slider-container">
                                            <div class="slider-range"></div>
                                            <div data-role="main" class="ui-content">
                                                    <div class="example">
                                                        <div class="slider-wrapper">
                                                            <input type="text" class="js-min-max-start" />
                                                        </div>
                                                    </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <button class="volume">
                                    <i class="fa fa-volume-off click-active " aria-hidden="true"></i>
                                    <i class="fa fa-volume-up click-hidden" aria-hidden="true"></i>
                                </button>
								<div class="select-time-end timeline selection-gif">
                                    <div class="selection">
                                        <div class="slider-container">
                                            <div class="slider-range"></div>
                                            <div data-role="main" class="ui-content">
                                                    <div class="example">
                                                        <div class="slider-wrapper">
                                                            <input type="text" class="js-min-max-start-1" />
                                                        </div>
                                                    </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                     </div>
